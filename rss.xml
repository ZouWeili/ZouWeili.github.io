&lt;span class=&quot;token keyword&quot;&gt;const&lt;/span&gt; bailRE &lt;span class=&quot;token operator&quot;&gt;=&lt;/span&gt; &lt;span class=&quot;token regex&quot;&gt;/[^\w.$]/&lt;/span&gt;
